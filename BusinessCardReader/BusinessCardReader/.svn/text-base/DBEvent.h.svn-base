#import <Foundation/Foundation.h>
#import "DBRow.h"

@interface DBEvent : DBRow

@property (strong, nonatomic) NSString *eventID;
@property (strong, nonatomic) NSString *eventName;
@property (strong, nonatomic) NSString *description;
@property (strong, nonatomic) NSString *launchDate;
@property (strong, nonatomic) NSString *endDate;
@property (strong, nonatomic) NSString *expiryDate;
@property (strong, nonatomic) NSString *city;
@property (strong, nonatomic) NSString *state;
@property (strong, nonatomic) NSString *country;
@property (nonatomic) BOOL isActive;
@property (strong, nonatomic) NSString *topicList;
@property (strong, nonatomic) NSString *followupList;
@property (strong, nonatomic) NSString *appData;

@property (strong, nonatomic) NSString *userID;
@property (strong, nonatomic) NSString *companyID;

@end
